        }
      },
      "TheArchive.Features.Accessibility.GlassLiquidOverride+GlassLiquidOverrideSettings.ApplyDisinfectButton": {
        "FSDisplayName": {
          "English": "Clean Screen",
          "Chinese": "清理屏幕",
          "French": null,
          "Italian": null,
          "German": null,
          "Spanish": null,
          "Russian": null,
          "Portuguese_Brazil": null,
          "Polish": null,
          "Japanese": null,
          "Korean": null
        },
        "FSDescription": {
          "English": "Applies the Disinfect Station visuals to clear any other liquids.\n\n(Might fix bad cases of liquid bug if spammed a bunch!)",
          "Chinese": "应用消毒站的可视化效果，清除任何其他液体。\n\n（如果频繁使用，可能修复严重的液体错误情况！）",
          "French": null,
          "Italian": null,
          "German": null,
